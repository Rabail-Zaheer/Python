in_city = "brisbane"

age = -10000

gurneets_score_on_code = 1000000.0

gurneetis10000 = False

print("in_city has a datatype",type (in_city))

print("age has a datatype",type (age))

print("gurneets_score_on_code has a datatype" ,type (gurneets_score_on_code))

print("gurneetis10000 has a datatype",type (gurneetis10000))